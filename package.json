{
  "author": "Troy Alford",
  "name": "axis-wiki",
  "version": "1.0.0",
  "description": "Node.js Wiki, based on React w/ES6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TroyAlford/axis-wiki.git"
  },
  "dependencies": {
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "body-parser": "^1.16.1",
    "brace": "^0.10.0",
    "bulma": "^0.3.1",
    "cheerio": "^0.22.0",
    "chokidar": "^1.6.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "del": "^2.2.2",
    "express": "^4.14.1",
    "fs-utils": "^0.7.0",
    "graceful-fs": "^4.1.11",
    "isomorphic-fetch": "^2.2.1",
    "jimp": "^0.2.27",
    "js-beautify": "^1.6.9",
    "js-cookie": "^2.1.3",
    "lodash": "^4.17.4",
    "mathjs": "^3.9.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "multer": "^1.3.0",
    "node-sass-glob-importer": "^3.0.2",
    "normalize.css": "^5.0.0",
    "prop-types": "^15.5.8",
    "q": "^1.4.1",
    "react": "^15.5.4",
    "react-ace": "^4.2.1",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.10.0",
    "react-helmet": "^4.0.0",
    "react-jsx-parser": "^0.0.13",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-tagsinput": "^3.14.0",
    "react-tinymce": "^0.5.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "superchild": "^0.1.10",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-jest": "^19.0.0",
    "babel-preset-react": "*",
    "babel-watch": "^2.0.6",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "foreman": "^2.0.0",
    "jest-cli": "^19.0.2",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-devtools": "^3.3.2",
    "rimraf": "^2.5.4",
    "source-map-explorer": "^1.3.3",
    "webpack": "^2.2.1"
  },
  "scripts": {
    "start": "yarn build-prod && yarn serve-prod",
    "test": "jest",
    "develop": "yarn watch & yarn serve-watched",
    "build": "yarn prebuild && (yarn build-css & yarn webpack)",
    "build-dev": "yarn build",
    "build-prod": "NODE_ENV=production yarn build",
    "build-css": "(yarn build-css-app & yarn build-css-dep)",
    "build-css-app": "node-sass styles/application.scss --output-style compressed --source-map true --source-map-embed --importer node_modules/node-sass-glob-importer/dist/cli.js -o build/css",
    "build-css-dep": "node-sass vendor/dependencies.scss --output-style compressed --source-map true --source-map-embed -o build/css ",
    "prebuild": "rimraf build/css/* && rimraf build/js/*",
    "serve": "babel-node server/server.js",
    "serve-prod": "NODE_ENV=production yarn serve",
    "serve-watched": "nodemon --watch ./server --watch ./config -e .js --exec 'yarn serve'",
    "watch": "SOURCEMAP=true yarn prebuild && (yarn watch-css & yarn webpack -- --watch)",
    "watch-css": "yarn watch-css-app & yarn watch-css-dep",
    "watch-css-app": "nodemon --watch ./styles -e scss --exec 'yarn build-css-app'",
    "watch-css-dep": "nodemon --watch ./vendor -e scss --exec 'yarn build-css-dep'"
  },
  "babel": {
    "plugins": [
      "lodash",
      "syntax-async-functions",
      "transform-object-rest-spread",
      "transform-regenerator"
    ],
    "presets": [
      "es2015",
      "react",
      "jest"
    ],
    "highlightCode": true
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-router/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/lodash/"
    ]
  }
}
