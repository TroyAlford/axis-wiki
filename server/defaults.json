{
  "facebook": {
    "application_id": "YOUR_APP_ID_HERE",
    "permissions": "public_profile,email"
  },
  "html": {
    "beautifier": {
      "end_with_newline": true,
      "eol": "\n",
      "indent_char": " ",
      "indent_size": 2,
      "indent_with_tabs": false,
      "max_preserve_newlines": 0,
      "wrap_line_length": 0
    }
  },
  "media": {
    "image_resize_width": 250
  },
  "navigation": {
    "default_links": [
      { "text": "Home", "url": "/page/home" },
      { "text": "Upload", "url": "/upload" }
    ]
  },
  "saving": {
    "throttle": 1000
  },
  "server": {
    "port": 8080
  },
  "storage": {
    "path": "../content"
  }
}